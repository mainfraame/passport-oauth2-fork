{
  "name": "passport-oauth2",
  "version": "2.0.0",
  "description": "OAuth 2.0 authentication strategy for Passport.",
  "keywords": [
    "passport",
    "auth",
    "authn",
    "authentication",
    "authz",
    "authorization",
    "oauth",
    "oauth2"
  ],
  "author": {
    "name": "Jared Hanson",
    "email": "jaredhanson@gmail.com",
    "url": "http://www.jaredhanson.net/"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/jaredhanson/passport-oauth2.git"
  },
  "bugs": {
    "url": "http://github.com/jaredhanson/passport-oauth2/issues"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/jaredhanson"
  },
  "license": "MIT",
  "licenses": [
    {
      "type": "MIT",
      "url": "http://opensource.org/licenses/MIT"
    }
  ],
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "preppy --silent",
    "depcheck": "depcheck .",
    "lint": "eslint --ignore-path .eslintignore src test --quiet",
    "test": "jest --config=jest.config.js --coverage --silent --verbose",
    "typecheck": "tsc --noEmit --incremental false"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.18.3",
    "nanoid": "^3.3.4",
    "oauth": "^0.9.15",
    "passport-strategy": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-typescript": "^7.17.12",
    "@babel/plugin-transform-runtime": "^7.18.5",
    "@types/jest": "^27.0.2",
    "@types/passport-strategy": "^0.2.35",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "chai": "^4.3.6",
    "chai-passport-strategy": "^3.0.0",
    "depcheck": "^1.4.3",
    "eslint": "^8.8.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.5.3",
    "import-sort-style-module": "^6.0.0",
    "jest": "^27.1.1",
    "preppy": "^11.0.2",
    "prettier": "^2.6.2",
    "prettier-plugin-import-sort": "^0.0.7",
    "typescript": "^4.7.4"
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  },
  "prettier": {
    "arrowParens": "always",
    "bracketSameLine": true,
    "bracketSpacing": true,
    "embeddedLanguageFormatting": "auto",
    "endOfLine": "auto",
    "htmlWhitespaceSensitivity": "strict",
    "jsxSingleQuote": true,
    "insertPragma": false,
    "quoteProps": "as-needed",
    "requirePragma": false,
    "proseWrap": "preserve",
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "none",
    "useTabs": false
  },
  "sideEffects": false
}
